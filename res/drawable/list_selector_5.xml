<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
			<solid
				android:color="#aaefefef"/>
			<stroke
				android:width="1px"
		        android:color="#666" />
			<corners
				android:radius="9dp"/>
		</shape>
    </item>
    <item>
        <shape>
			<solid
				android:color="#aaffffff"/>
			<stroke
				android:width="1px"
		        android:color="#999" />
			<corners
				android:radius="9dp"/>
		</shape>
    </item>

</selector>
