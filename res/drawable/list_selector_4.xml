<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape>
			<solid android:color="#ddd" />
		    <stroke
				android:width="1px"
		        android:color="#666" />
		</shape>
	   </item>
	<item>
		<shape>
			<solid android:color="#fff" />
		    <stroke
				android:width="1px"
		        android:color="#999" />
		</shape>
	</item>
   
</selector>
